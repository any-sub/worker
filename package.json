  "license": "MIT",
  "dependencies": {
    "@any-sub/worker-transport": "0.0.17",
    "@tsed/core": "7.67.6",
    "@tsed/di": "7.67.6",
    "@tsed/logger": "6.7.5",
    "@types/node": "20.12.7",
    "dotenv": "16.4.5",
    "handlebars": "4.7.8",
    "jsdom": "22.1.0",
    "jsonpath": "1.1.1",
    "node-fetch": "2.6.7",
    "socket.io-client": "4.7.5",
    "string-hash": "1.1.3",
    "znv": "0.4.0",
    "zod": "3.23.0"
  },
  "devDependencies": {
    "@tsed/common": "7.67.6",
    "@types/chance": "1.1.6",
    "@types/jest": "29.5.12",
    "@types/jsdom": "21.1.6",
    "@types/jsonpath": "0.2.4",
    "@types/node-fetch": "2",
    "@types/string-hash": "1.1.3",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "barrelsby": "2.8.1",
    "chance": "1.1.11",
    "cross-env": "7.0.3",
    "eslint": "9.1.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "5.1.3",
    "jest": "29.7.0",
    "jest-mock-server": "0.1.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-jest": "29.1.2",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "5.4.5"
  },
  "scripts": {
    "clean": "rimraf src/generated",
    "build": "yarn barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "test:lint": "eslint \"**/*.{ts,js}\"",
    "test:lint:fix": "eslint \"**/*.{ts,js}\" --fix",
    "prettier": "prettier \"**/*.{ts,js,json,md,yml,yaml}\" --write"
  }
}
