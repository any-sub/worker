{
  "name": "@any-sub/worker",
  "version": "0.0.1",
  "main": "src/index.js",
  "license": "MIT",
  "dependencies": {
    "@tsed/di": "7.27.2",
    "@tsed/logger": "6.5.0",
    "@types/node": "20.1.1",
    "barrelsby": "2.7.0",
    "dotenv": "16.0.3",
    "socket.io-client": "4.6.1",
    "znv": "0.3.2",
    "zod": "3.21.4"
  },
  "devDependencies": {
    "@types/jest": "29.5.1",
    "@typescript-eslint/eslint-plugin": "5.59.5",
    "@typescript-eslint/parser": "5.59.5",
    "cross-env": "7.0.3",
    "eslint": "8.40.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "jest": "29.5.0",
    "prettier": "2.8.8",
    "rimraf": "5.0.0",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "typescript": "5.0.4"
  },
  "scripts": {
    "clean": "rimraf src/generated",
    "build": "yarn barrels && tsc --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "yarn barrels && tsnd --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn test:lint && yarn test:coverage",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:coverage": "yarn test:unit",
    "test:lint": "eslint \"**/*.{ts,js}\"",
    "test:lint:fix": "eslint \"**/*.{ts,js}\" --fix",
    "prettier": "prettier \"**/*.{ts,js,json,md,yml,yaml}\" --write"
  }
}
