{
  "extends": ["config:base"],
  "labels": ["dependencies"],
  "ignoreDeps": ["node-fetch"],
  "packageRules": [
    {
      "matchUpdateTypes": ["minor", "patch", "pin", "digest"],
      "automerge": true
    },
    {
      "groupName": "Ts.ED",
      "matchPackagePrefixes": ["@tsed/"]
    }
  ],
  "baseBranches": ["develop"]
}
